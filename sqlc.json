{
    "version": "2",
    "sql": [
      {
        "schema": "sql/migrations",
        "queries": "sql/queries",
        "engine": "mysql",
        "gen": {
          "go": {
            "package": "database",
            "out": "internal/infra/database",
            "emit_interface": true,
            "overrides": [
              {
                "go_type": "github.com/shopspring/decimal.Decimal",
                "db_type": "decimal"
              },
              {
                "go_type": "github.com/shopspring/decimal.Decimal",
                "db_type": "decimal"
              }        
            ]
          }
        }
      }
    ]
  }